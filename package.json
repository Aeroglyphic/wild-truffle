{
  "name": "wild-truffle",
  "version": "1.0.0",
  "description": "Experiment: multi-repo CI using meta",
  "main": "index.js",
  "scripts": {
    "ci": "./scripts/ci.sh",
    "install:ci": "./scripts/install_CI.sh",
    "install:local": "./scripts/install_local.sh",
    "test": "ZEPPELIN=true ./scripts/test.sh",
    "test:zeppelin": "ZEPPELIN=true ./scripts/test.sh",
    "test:aragon": "ARAGON=true ./scripts/test.sh",
    "test:colony": "COLONY=true ./scripts/test.sh",
    "test:ci": "./scripts/test.sh",
    "build": "npm run build:truffle && npm run build:ganache",
    "build:truffle": "webpack --config ./webpack/truffle_cli.webpack.config.js",
    "build:ganache": "webpack --config ./webpack/ganache_cli.webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trufflesuite/wild-truffle.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trufflesuite/wild-truffle.git"
  },
  "homepage": "https://github.com/trufflesuite/wild-truffle#README.md",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "fs-extra": "^4.0.0",
    "ganache-core": "^2.1.0",
    "js-scrypt": "^0.2.0",
    "meta-npm": "^0.0.22",
    "meta-pkgs": "^0.2.0",
    "on-build-webpack": "^0.1.0",
    "prepend-file": "^1.3.1",
    "shebang-loader": "0.0.1",
    "stream-buffers": "^3.0.1",
    "truffle-core": "^4.1.3",
    "web3": "^0.20.1",
    "webpack": "^2.5.1"
  }
}
